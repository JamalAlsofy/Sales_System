<Window x:Class="ONEX_Seles.Home"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ONEX_Seles"
        mc:Ignorable="d" Height="400" Width="400" WindowState="Maximized" IsEnabled="False" WindowStartupLocation="CenterScreen" ResizeMode="NoResize" AllowsTransparency="True" WindowStyle="None">
    <Window.Background>
        <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
            <GradientStop Color="Black" Offset="0.015"/>
            <GradientStop Color="White" Offset="1"/>
        </LinearGradientBrush>
    </Window.Background>
    <Grid>
        <Grid x:Name="gMenu">
            <Grid.Background>
                <RadialGradientBrush>
                    <GradientStop Color="Black" Offset="0.754"/>
                    <GradientStop Color="White" Offset="1"/>
                    <GradientStop Color="#FF6D6D6D" Offset="0.886"/>
                </RadialGradientBrush>
            </Grid.Background>
        </Grid>
    </Grid>
</Window>
